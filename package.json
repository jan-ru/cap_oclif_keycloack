{
  "name": "financial-reports-cli",
  "description": "A new CLI generated with oclif",
  "version": "0.1.1",
  "author": "jan-ru",
  "bin": {
    "financial-reports-cli": "./bin/run.js"
  },
  "bugs": "https://github.com/jan-ru/financial-reports-cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@sap-cloud-sdk/http-client": "^4.3.1",
    "@sap-cloud-sdk/odata-v4": "^4.3.1",
    "@sap-cloud-sdk/util": "^4.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@types/node": "^20.19.28",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "audit-ci": "^7.1.0",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "oclif": "^4",
    "prettier": "^3.7.4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/jan-ru/financial-reports-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "financial-reports-cli",
    "dirname": "financial-reports-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "jan-ru/financial-reports-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "version": "oclif readme && git add README.md",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "validate": "npm run lint && npm test && npm run build"
  },
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  }
}
